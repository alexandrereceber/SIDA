<IfModule mod_rewrite.c>
Options -Indexes

#Pro√≠be acesso direto aos arquivos via URL. ex.: http://10.56.32.78/sistemaonline/Controller/Controllers.php
RewriteCond %{QUERY_STRING} URL=ControladorTabelas/
RewriteRule (.*) TBL/Tabelas.php?p=%{QUERY_STRING} [L]

RewriteCond %{REQUEST_URI} .*
RewriteRule (.*) - [F]
</IfModule>

